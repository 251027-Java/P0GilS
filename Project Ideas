Tables:
class Card
    String id,
    name,
    setId,
    setName,
    rarity,
    smallImg,

class Set
    String id,
    name,
    LocalDate,
    releaseDate

class Type
    int id
    String name



class Deck
    int id,
    String name,
    Map<String,Integer> cardQuantities // cardId -> qty

class PriceSnapshot
    long id,
     String cardId,
     String source,
     BigDecimal marketUsd,
     BigDecimal trendEur,
     Instant capturedAt

